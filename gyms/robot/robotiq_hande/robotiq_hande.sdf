    <!--
    Defines an invisible volume for the purpose of the SimpleGripperPlugin:
    When an object collides with that volume, it is considered "grasped".

    Please note:
    * Inertial values should be inconsequential
    * The pose of the volume may be eyeballed in MeshLab
    -->
    <link name="sticky_link">
      <pose>0.0 0.0 0.128 0 0 0</pose>
      <inertial>
        <mass>0.001</mass>
        <pose>0 0 0 0 0 0</pose>
        <inertia>
          <ixx>0.00000160833269</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00000160833269</iyy>
          <iyz>0</iyz>
          <izz>0.0000032</izz>
        </inertia>
      </inertial>
      <collision name="collision_grip">
        <pose>0 0 0.0 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.02</radius>
            <length>0.04</length>
          </cylinder>
        </geometry>
        <surface>
          <contact>
            <collide_without_contact>true</collide_without_contact>
          </contact>
        </surface>
      </collision>
    </link>
    <joint name="sticky_link_joint" type="fixed">
      <parent>gripper_body</parent>
      <child>sticky_link</child>
    </joint>

    <joint name="finger1" type="prismatic">
      <parent>gripper_body</parent>
      <child>gripper_finger1</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>0.025</upper>
        </limit>
       </axis>
    </joint>
    <joint name="finger2" type="prismatic">
      <parent>gripper_body</parent>
      <child>gripper_finger2</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 -1 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>0.025</upper>
        </limit>
       </axis>
    </joint>

